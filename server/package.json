{
  "name": "forumpage-patchnotes-admin-server",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^2.0.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
